{
  "name": "csabay",
  "version": "1.0.0",
  "description": "A website Craigslist/eBay type website where people can publicize selling information and contact information for sellers",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "client": "cd client && npm start",
    "dev": "concurrently \"nodemon server.js\" \"npm run client\" ",
    "build": "cd client && npm run build",
    "install-client": "cd client && npm install",
    "heroku-postbuild": "npm run install-client && npm run build",
    "devStart": "nodemon server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shida-yang/CSABay.git"
  },
  "author": "",
