  "bugs": {
    "url": "https://github.com/shida-yang/CSABay/issues"
  },
  "homepage": "https://github.com/shida-yang/CSABay#readme",
  "keywords": [],
  "dependencies": {
    "aws-sdk": "^2.879.0",
    "concurrently": "^6.0.1",
    "cookie": "^0.4.1",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "mongoose": "^5.12.2",
    "parser": "^0.1.4",
    "randomstring": "^1.1.5"
  },
  "devDependencies": {
    "nodemon": "^2.0.7"
  }
